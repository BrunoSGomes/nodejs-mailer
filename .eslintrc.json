{
    "env": {
        "node": true,
        "es2021": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "indent": "off",
        "no-param-reassign": "off",
        "import/extensions": "off",
        "no-console": "off"
    }
}